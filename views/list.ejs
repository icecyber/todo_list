<%- include("header") -%>

    <div class="box" id="heading">
      <h1><%= day %></h1>  
    </div>
    <div class="box">
        <ul>
            <% for (i =0; i<itemsLists.length; i++){ %>
                <div class="item">
                    <input type="checkbox">
                    <p><%= itemsLists[i] %></p>
                </div>
            <% } %>
        </ul>
        <form class="item" action="/" method="post">
            <input type="text" name="newLists" placeholder="New Item">
            <button type="submit">+</button>
        </form>
    </div>
    
<%- include("footer") -%> 